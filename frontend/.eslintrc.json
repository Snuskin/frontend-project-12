{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react-hooks/recommended", "plugin:react/recommended", "airbnb", "plugin:prettier/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": { 
    "react/react-in-jsx-scope": "off", 
    "import/order": "off", "prettier/prettier": "off", 
    "no-shadow": "off", "import/extensions": "off", 
    "jsx-a11y/label-has-associated-control": "off",
    "react/function-component-definition": "off",
    "react/button-has-type": "off"}
}
